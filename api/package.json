{
  "name": "api",
  "version": "0.0.1",
  "scripts": {
    "generatedb": "npx prisma generate",
    "build": "rm -rf dist && ./node_modules/.bin/tsc -p ./tsconfig.json",
    "buildprod": "rm -rf dist && ./node_modules/.bin/tsc -p ./tsconfig.json",
    "dev": "NODE_ENV=development nodemon -e ts,json --watch ./src/ --watch .env --exec 'yarn build && node ./dist/index.js'",
    "start": "node ./dist/index.js",
    "clean": "rm -rf node_modules && rm -rf .turbo && rm -rf dist"
  },
  "dependencies": {
    "@fastify/cookie": "^7.0.0",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.1",
    "fastify": "^4.2.1",
    "fastify-plugin": "^4.0.0",
    "jose": "^4.8.3",
    "nodemailer": "^6.7.7",
    "prisma": "^4.0.0",
    "zod": "^3.17.3",
    "zod-to-json-schema": "^3.17.0"
  },
  "devDependencies": {
    "@prisma/client": "^4.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/nodemailer": "^6.4.4",
    "nodemon": "^2.0.19",
    "tsc-watch": "^5.0.3",
    "tsconfig": "*",
    "typescript": "^4.7.4"
  }
}
