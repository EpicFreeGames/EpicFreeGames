{
  "name": "@efg/sender",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rm -rf dist && ./node_modules/.bin/tsc -p ./tsconfig.json",
    "dev": "nodemon -q -e ts,json --ignore ./dist --watch ./ --exec 'yarn build && node --experimental-specifier-resolution=node ./dist/index.js'",
    "start": "node ./dist/index.js",
    "clean": "rm -rf node_modules && rm -rf .turbo && rm -rf dist"
  },
  "dependencies": {
    "@efg/configuration": "*",
    "@efg/embeds": "*",
    "@efg/logger": "*",
    "@efg/shared-utils": "*",
    "@efg/types": "*",
    "discord-api-types": "0.37.8",
    "express": "^4.17.2",
    "zod": "^3.19.0"
  },
  "devDependencies": {
    "@efg/tsconfig": "*",
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.19",
    "typescript": "4.7.4"
  }
}
