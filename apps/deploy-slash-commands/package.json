{
  "name": "deploy-slash-commands",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "private-build": "rm -rf dist && ./node_modules/.bin/tsc -p ./tsconfig.json",
    "predepl": "yarn private-build",
    "predepl-g": "yarn private-build",
    "depl": "node --experimental-specifier-resolution=node ./dist/index.js",
    "depl-g": "node --experimental-specifier-resolution=node ./dist/index.js -g"
  },
  "dependencies": {
    "axios": "^0.26.0",
    "typescript": "^4.5.5",
    "discord-api-types": "^0.27.1",
    "discord.js": "^13.6.0",
    "shared": "*",
    "config": "*"
  },
  "devDependencies": {
    "tsconfig": "*"
  }
}
