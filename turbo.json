{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"],
      "env": ["ENV", "VERSION"]
    },
    "buildprod": {
      "dependsOn": ["^build", "^buildprod"],
      "outputs": ["dist/**", ".next/**"],
      "env": ["ENV", "VERSION"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"],
      "env": ["ENV", "VERSION"]
    },
    "start": {
      "dependsOn": ["^build", "^buildprod", "build", "^start"],
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["^build", "build", "^typecheck"],
      "cache": false
    }
  }
}
