{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "buildprod": {
      "dependsOn": ["^buildprod"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "dependsOn": ["build"],
      "cache": false
    },
    "reg": {
      "dependsOn": ["build"]
    },
    "reg-g": {
      "dependsOn": ["build"]
    },
    "clean": {
      "cache": false
    },
    "start": {
      "dependsOn": ["^start"],
      "cache": false
    }
  }
}
