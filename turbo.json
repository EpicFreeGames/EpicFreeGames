{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {
      "dependsOn": ["build"],
      "cache": false
    },
    "reg": {
      "dependsOn": ["build"]
    },
    "reg-g": {
      "dependsOn": ["build"]
    },
    "clean": {
      "cache": false
    },
    "dev:front": {
      "dependsOn": ["^dev:front"],
      "cache": false
    },
    "build:front": {
      "dependsOn": ["^build:front"],
      "cache": false
    },
    "start:front": {
      "dependsOn": ["^start:front"],
      "cache": false
    }
  }
}
