{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "buildprod": {
      "dependsOn": ["^buildprod"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "clean": { "cache": false },
    "start": {
      "dependsOn": ["^build", "^buildprod", "build", "^start"],
      "cache": false
    }
  }
}
