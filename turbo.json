{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "buildprod": {
      "dependsOn": ["^build", "^buildprod"],
      "outputs": ["dist/**", ".next/**"]
    },
    "dev": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "start": {
      "dependsOn": ["^build", "^buildprod", "build", "^start"],
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["^build", "build", "^typecheck"],
      "cache": false
    }
  }
}
