---
import IndexPage from "../../components/IndexPage.astro";
import { webLanguages } from "../../i18n/languages";
import "../../styles.css";

export async function getStaticPaths() {
    const paths = Object.values(webLanguages).map((lang) => ({
        params: { lang: lang.code },
    }));

    return paths;
}
