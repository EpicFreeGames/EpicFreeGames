{
  "name": "@efg/discord-rest-proxy",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "rm -rf dist && ./node_modules/.bin/tsc -p ./tsconfig.json",
    "start": "node --experimental-specifier-resolution=node ./dist/index.js",
    "devstart": "yarn build && yarn start",
    "dev": "nodemon -q -e ts,json --ignore ./dist --watch ./ --exec yarn devstart"
  },
  "dependencies": {
    "@efg/configuration": "*",
    "@efg/shared-utils": "*",
    "@efg/logger": "*",
    "axios": "^0.27.2",
    "axios-rate-limit": "^1.3.0",
    "express": "^4.17.2"
  },
  "devDependencies": {
    "@efg/tsconfig": "*",
    "@types/express": "^4.17.13",
    "nodemon": "^2.0.19",
    "typescript": "4.7.4"
  }
}
