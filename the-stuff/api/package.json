{
  "name": "epicfreegames-api",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && ./node_modules/.bin/tsc -p ./tsconfig.json",
    "dev": "nodemon -q -e ts,json --watch ./src/ --watch ./prisma --exec 'yarn build && node ./dist/index.js'",
    "typecheck": "tsc --skipLibCheck --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,json}\"",
    "db": " yarn dotenv -e ../.env-dev yarn --cwd ./api prisma studio",
    "dbpush": "cd ./api/ && yarn dotenv -e ../.env-dev yarn prisma db push",
    "dbreset": "cd ./api/ && yarn dotenv -e ../.env-dev yarn prisma migrate reset && cd ../ && yarn dbpush"
  },
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "ioredis": "^5.2.2",
    "jose": "^4.8.3",
    "uuid": "^8.3.2",
    "ws": "^8.8.1",
    "zod": "^3.17.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/uuid": "^8.3.4",
    "@types/ws": "^8.5.3",
    "dotenv-cli": "^6.0.0",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "prisma": "^4.0.0",
    "typescript": "^4.7.4"
  }
}
