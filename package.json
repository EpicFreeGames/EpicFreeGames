{
  "name": "freegames-bot",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "postclean": "rm -rf node_modules",
    "clean": "turbo run clean",
    "reg": "turbo run reg",
    "reg-g": "turbo run reg-g",
    "devup": "docker-compose -p dev up -d --build",
    "devdown": "docker-compose -p dev down",
    "produp": "docker-compose -p prod up -d --build",
    "proddown": "docker-compose -p prod down",
    "dbup": "docker-compose -p dev up -d efg-db --build",
    "dbdown": "docker-compose -p dev down -d efg-db --build"
  },
  "devDependencies": {
    "turbo": "^1.1.2"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "yarn@1.22.17"
}
