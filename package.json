{
  "name": "freegames-bot",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "devr": "yarn clean && yarn && yarn build && yarn dev",
    "dev:front": "yarn build && turbo run dev:front",
    "build:front": "turbo run build:front",
    "start:front": "turbo run start:front",
    "postclean": "rm -rf node_modules",
    "clean": "turbo run clean",
    "reg": "turbo run reg",
    "reg-g": "turbo run reg-g",
    "up": "docker-compose up -d --build",
    "down": "docker-compose down"
  },
  "devDependencies": {
    "turbo": "^1.2.4"
  },
  "engines": {
    "npm": ">=8.5.5",
    "node": ">=16.5.0"
  },
  "packageManager": "yarn@1.22.19"
}
