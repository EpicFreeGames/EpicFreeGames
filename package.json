{
  "name": "efg-monorepo",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "clean": "rm -rf ./**/**/dist ./**/**/node_modules ./**/**/.next ./**/**/.turbo ./node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,json}\"",
    "f::typecheck": "turbo run typecheck --filter=website --filter=internal-dashboard",
    "api::dev": "./node_modules/.bin/dotenv -e .env.development -- turbo run dev --filter=api...",
    "ie::dev": "./node_modules/.bin/dotenv -e .env.development -- turbo run dev --filter=interactions-endpoint...",
    "drp::dev": "./node_modules/.bin/dotenv -e .env.development -- turbo run dev --filter=discord-rest-proxy...",
    "web::dev": "./node_modules/.bin/dotenv -e .env.development -- turbo run dev --filter=website...",
    "sender::dev": "./node_modules/.bin/dotenv -e .env.development -- turbo run dev --filter=sender...",
    "idash::dev": "./node_modules/.bin/dotenv -e .env.development -- turbo run dev --filter=internal-dashboard...",
    "cmds": "./node_modules/.bin/dotenv -e .env.development -- turbo run start --filter=slash-commands...",
    "db::update": "cd apps/api/ && ../../node_modules/.bin/dotenv -e ../../.env.development -- yarn prisma db push",
    "db::studio": "cd apps/api/ && ../../node_modules/.bin/dotenv -e ../../.env.development -- yarn prisma studio"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "dotenv-cli": "^6.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "turbo": "1.4.4"
  },
  "engines": {
    "npm": ">=8.6.0",
    "node": ">=16.0.0"
  },
  "packageManager": "yarn@1.22.19"
}
